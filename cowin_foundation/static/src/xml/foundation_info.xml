<?xml version="1.0" encoding="UTF-8"?>

<templates xml:space="preserve">

    <t t-name="foundation_info_templ">
        <div id="foundation_info_container">
            <div class="project_header">
                <h2><t t-esc="result.name"/></h2>
            </div>
            <div style="width:100%;height:10px;background:rgba(242, 242, 242, 1)"></div>
            <ul id="foundation_tab" class="nav nav-tabs">
                <li  class="active"><a href="#foundation_infos" data-toggle="tab">基金情况表</a></li>
                <li><a class="list_of_contributors" href="#list_of_contributors" data-toggle="tab">出资人列表</a></li>
                <li><a href="#" data-toggle="tab">GP团队</a></li>
                <li><a class="manage_company_info" href="#manage_company_info" data-toggle="tab">管理公司情况</a></li>
                <li><a href="#" data-toggle="tab">中介机构情况</a></li>
                <li><a href="#" data-toggle="tab">设置</a></li>
            </ul>
            <div id="foundation_tab_content" class="tab-content">
                <!--基金情况表-->
                <div class="tab-pane fade in active" id="foundation_infos">
                    <div class="basic_infos_wrap">
                        <table style="margin-right: 180px;">
                            <tbody>
                                <tr><td>基金编号</td><td><t t-esc="result.foundation_number"/></td></tr>
                                <tr><td>注册地址</td><td><t t-esc="result.registered_address"/></td></tr>
                                <tr><td>基金类型</td><td><t t-esc="result.foundation_type"/></td></tr>
                                <tr><td>投资领域</td><td><t t-esc="result.investment_field"/></td></tr>
                                <tr><td>投资期限（年）</td><td><t t-esc="result.term_of_investment"/></td></tr>
                            </tbody>
                        </table>
                        <table>
                            <tbody>
                                <tr><td>投资起始日</td><td><t t-esc="result.start_date_of_investment"/></td></tr>
                                <tr><td>基金注册时间</td><td><t t-esc="result.registered_foundation_date"/></td></tr>
                                <tr><td>基金首期出资时间</td><td><t t-esc="result.first_investment_time_of_the_fund"/></td></tr>
                                <tr><td>基金投资金额（万元）</td><td><t t-esc="result.fund_investment_amonut"/></td></tr>
                                <tr><td>证券账户</td><td><t t-esc="result.securities_account"/></td></tr>
                            </tbody>
                        </table>
                        <div class="edit_foundation"><i class="fa fa-edit"></i> 编辑</div>
                    </div>
                    <div class="foundation_info_labels_wrap">
                        <div class="info_label">
                            <div style="width: 200px;">
                                <div class="digital" style="color:#FF6600;"><t t-esc="result.fund_recognition_scale"/></div>
                                <div>基金认缴规模<span class="info_label_danwei">(万元)</span></div>
                            </div>
                            <table>
                                <tbody>
                                    <tr><td>目前基金到位资金额<span class="info_label_danwei">(万元)</span></td><td><t t-esc="result.funds_in_place_at_present_amount"/></td></tr>
                                    <tr><td>目前基金到位资金比率</td><td><t t-esc="result.funds_in_place_at_present_ratio"/></td></tr>
                                </tbody>
                            </table>
                        </div>
                        <div class="info_label">
                            <div style="width: 200px;">
                                <div class="digital"><t t-esc="result.state_investor_commitment_capital_con_amount"/></div>
                                <div>国有出资人承诺出资<span class="info_label_danwei">(万元)</span></div>
                            </div>
                            <table>
                                <tbody>
                                    <tr><td>目前其它国有出资人到位资金额<span class="info_label_danwei">(万元)</span></td><td><t t-esc="result.current_state_investor_commitment_capital_con_amount"/></td></tr>
                                    <tr><td>目前其它国有出资人到位比率</td><td><t t-esc="result.current_state_investor_commitment_capital_con_amount_ratio"/></td></tr>
                                </tbody>
                            </table>
                        </div>
                        <div class="info_label">
                            <div style="width: 200px;">
                                <div class="digital"><t t-esc="result.commitment_con_by_social_contributors_amount"/></div>
                                <div>社会出资人承诺出资<span class="info_label_danwei">(万元)</span></div>
                            </div>
                            <table>
                                <tbody>
                                    <tr><td>目前社会出资人到位资金额<span class="info_label_danwei">(万元)</span></td><td><t t-esc="result.current_commitment_con_by_social_contributors_amount"/></td></tr>
                                    <tr><td>目前社会出资人到位比率</td><td><t t-esc="result.current_commitment_con_by_social_contributors_amount_ratio"/></td></tr>
                                </tbody>
                            </table>
                        </div>
                        <div class="info_label">
                            <div style="width: 200px;">
                                <div class="digital"><t t-esc="result.team_commitment_capital_con_amount_GP"/></div>
                                <div>GP团队承诺出资<span class="info_label_danwei">(万元)</span></div>
                            </div>
                            <table>
                                <tbody>
                                    <tr><td>目前GP团队到位资金额<span class="info_label_danwei">(万元)</span></td><td><t t-esc="result.current_team_commitment_capital_con_amount_GP"/></td></tr>
                                    <tr><td>目前GP团队到位比率</td><td><t t-esc="result.current_team_commitment_capital_con_amount_GP_ratio"/></td></tr>
                                </tbody>
                            </table>
                        </div>

                    </div>
                    <div class="foundation_files_wrap">
                        <h3>基金募资报告</h3>
                        <div>
                            <t t-foreach="result.fund_raising_report_ids" t-as="file">
                                <a t-attf-src="/web/content/#{file.id}?download=true"><t t-esc="file.name"/></a>
                            </t>
                        </div>
                        <h3>基金备案证书</h3>
                        <div>
                            <t t-foreach="result.fund_record_certificate_ids" t-as="file">
                                <a t-attf-href="/web/content/#{file.id}?download=true"><t t-esc="file.name"/></a>
                            </t>
                        </div>
                        <h3>协议</h3>
                        <div>
                            <t t-foreach="result.agreement_ids" t-as="file">
                                <a t-attf-href="/web/content/#{file.id}?download=true"><t t-esc="file.name"/></a>
                            </t>
                        </div>
                        <h3>政府审批材料</h3>
                        <div>
                            <t t-foreach="result.government_approval_materials_ids" t-as="file">
                                <a t-attf-href="/web/content/#{file.id}?download=true"><t t-esc="file.name"/></a>
                            </t>
                        </div>
                        <h3>基金半年度报告</h3>
                        <div>
                            <t t-foreach="result.semi_annual_report_of_the_fund_ids" t-as="file">
                                <a t-attf-href="/web/content/#{file.id}?download=true"><t t-esc="file.name"/></a>
                            </t>
                        </div>
                    </div>
                </div>
                <!--出资人列表-->
                <div class="tab-pane fade" id="list_of_contributors">

                </div>
                <!--管理公司情况-->
                <div class="tab-pane fade" id="manage_company_info">

                </div>
            </div>
        </div>
    </t>

    <!--出资人列表模板-->
    <t t-name="list_of_contributors_templ">
        <div class="add_new_contributor"><i class="fa fa-plus"></i> 新增出资人</div>
        <table id="table"></table>
    </t>

    <!--管理公司情况模板-->
    <t t-name="manage_company_info_templ">
        <div class="edit_manage_company_info">
            <i class="fa fa-edit"></i>
            编辑
        </div>
        <div class="manage_company_basic_info_wrap">
            <div>
                <div>基金管理公司名称</div>
                <div>基金管理公司通讯地址</div>
                <div>邮编</div>
            </div>
            <div>
                <div></div>
                <div></div>
                <div></div>
            </div>
        </div>
    </t>
</templates>