<odoo>
    <data>
        <!-- 出资人信息 -->
        <record id="cowin_foundation_sponsor_form" model="ir.ui.view">
            <field name="name">cowin_foudation.sponsor form</field>
            <field name="model">cowin_foudation.sponsor</field>
            <field name="arch" type="xml">
              <form create="0" delete="0">
                  <sheet>
                      <group>
                            <field name="name" required="1"/>
                            <field name="registered_address" required="1"/>
                            <field name="capital_contribution" required="1"/>
                            <field name="capital_ratio" readonly="1"/>
                            <field name="amount_of_payment" readonly="1"/>
                            <field name="amount_of_payment_ratio" readonly="1"/>
                            <field name="the_nature_of_the_investor" required="1"/>
                            <field name="institutional_investor" readonly="1"/>
                            <field name="mailing_address_of_shareholders" required="1"/>
                            <field name="contract_person" required="1"/>
                            <field name="contract_phone" required="1"/>
                            <field name="ID_number" required="1"/>
                            <field name="contract_email" required="1"/>
                      </group>
                  </sheet>

                  <footer>
                        <button type="object" name="button_save" string="Save"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                  </footer>
              </form>
            </field>
        </record>


    </data>
</odoo>