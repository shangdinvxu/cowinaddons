<odoo>
    <data>
        <!-- GP(普通合伙)信息 -->
        <record id="cowin_foundation_sponsor_gp_form" model="ir.ui.view">
            <field name="name">cowin_foundation.sponsor_gp form</field>
            <field name="model">cowin_foundation.sponsor_gp</field>
            <field name="arch" type="xml">
              <form>
                  <sheet>
                      <group>
                        <field name="name" required="1"/>
                        <field name="registered_address" required="1"/>
                        <field name="capital_contribution" required="1"/>
                        <field name="capital_ratio" readonly="1"/>
                        <field name="amount_of_payment" required="1"/>
                        <field name="amount_of_payment_ratio" readonly="1"/>
                        <field name="the_nature_of_the_investor" required="1"/>
                        <field name="institutional_investor" required="1"/>
                        <field name="mailing_address_of_shareholders" required="1"/>
                        <field name="contract_person" required="1"/>
                        <field name="contract_phone" readonly="1"/>
                        <field name="ID_number" readonly="1"/>
                        <field name="contract_email" readonly="1"/>
                      </group>
                      
                  </sheet>
                  
                  
                  <footer>
                        <button type="object" name="button_save" string="Save"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                  </footer>
              </form>
            </field>
        </record>


    </data>
</odoo>