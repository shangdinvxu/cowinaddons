<odoo>
    <data>
        <!-- 基金管理公司 -->
        <record id="cowin_foundation_management_company_form" model="ir.ui.view">
            <field name="name">cowin_foudation.management_company form</field>
            <field name="model">cowin_foudation.management_company</field>
            <field name="arch" type="xml">
              <form create="0" delete="0">
                  <sheet>
                        <group>
                            <field name="name" required="1"/>
                            <field name="management_company_adress" readonly="1"/>
                            <field name="zip_code" readonly="1"/>
                            <field name="management_company_chairman" required="1"/>
                            <field name="contract_phone_for_chairman" required="1"/>
                            <field name="management_company_manager" required="1"/>
                            <field name="contract_phone_for_manager" required="1"/>
                            <field name="management_company_daily_contact" required="1"/>
                            <field name="contract_phone_for_daily_contact" required="1"/>
                        </group>
                  </sheet>

                  <footer>
                        <button type="object" name="button_save" string="保存"/>
                        <button string="取消" class="btn-default" special="cancel"/>
                  </footer>
              </form>
            </field>
        </record>


    </data>
</odoo>