<odoo>
    <data>
        <!-- 尽调报告-->


           <!--尽调报告的form视图-->
        <record model="ir.ui.view" id="subt_dispatch_report_form">
            <field name="name">cowin_project.subt_dispatch_report form</field>
            <field name="model">cowin_project.subt_dispatch_report</field>
            <field name="arch" type="xml">
                <form string="Fiscal year" create="false" edit="false" delete="false">
                    <header>
                        <button class="review" type="object" name="button_approval_flow_info"  args="[55 ,66, 77]"  string="审核" style="background-color:#00B0DD;border-color:#00B0DD;"/>
                    </header>
                    <sheet>
                        <group>
                            <field name="dispatch_report" widget="many2many_binary" required="1"/>
                            <field name="attachment_note"/>
                        </group>


                        <group>

                                <field name="sub_pro_approval_flow_settings_record_ids" readonly="1">
                                       <tree >
                                         <field name="approval_person_id" readonly="1" options='{"no_open":True}'/>
                                         <field name="approval_role_id" readonly="1"/>
                                         <field name="approval_result" readonly="1"/>
                                         <field name="approval_opinion" readonly="1"/>
                                       </tree>
                                   </field>
                            </group>


                        <group>
                            <!--<button type="object" name="button_approval_flow_info"  args="[55 ,66, 77]"  string="审核"/>-->
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


        <record model="ir.ui.view" id="subt_dispatch_report_form_no_button">
            <field name="name">cowin_project.subt_dispatch_report form</field>
            <field name="model">cowin_project.subt_dispatch_report</field>
            <field name="arch" type="xml">
                <form string="Fiscal year" create="false" edit="false" delete="false">
                    <sheet>
                        <group>
                            <field name="dispatch_report" widget="many2many_binary" required="1"/>
                            <field name="attachment_note"/>
                        </group>


                        <group>

                                <field name="sub_pro_approval_flow_settings_record_ids" readonly="1">
                                       <tree >
                                         <field name="approval_person_id" readonly="1" options='{"no_open":True}'/>
                                         <field name="approval_role_id" readonly="1"/>
                                         <field name="approval_result" readonly="1"/>
                                         <field name="approval_opinion" readonly="1"/>
                                       </tree>
                                   </field>
                            </group>

                    </sheet>
                </form>
            </field>
        </record>

    </data>
</odoo>