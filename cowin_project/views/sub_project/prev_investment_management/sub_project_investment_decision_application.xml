<odoo>
    <data>


        <!-- 投资决策申请-->
        <!--投资决策申请的form视图-->
        <record model="ir.ui.view" id="sub_invest_decision_app">
            <field name="name">cowin_project.sub_invest_decision_app form</field>
            <field name="model">cowin_project.sub_invest_decision_app</field>
            <field name="arch" type="xml">
                <form string="Fiscal year">
                    <group>
                        <field name="name"/>
                        <field name="project_number"/>
                        <field name="invest_manager_id"/>
                        <!--投资基金-->
                        <field name="foundation_id"/>
                        <field name="the_amount_of_investment"/>
                        <field name="ownership_interest"/>
                        <field name="round_financing_id"/>
                        <field name="the_amount_of_financing"/>
                        <!---->
                        <field name="company_investment_role"/>
                        <field name="decision_file_list" widget="many2many_binary"/>
                        <field name="investment_decision_Committee_held_time"/>

                    </group>

                </form>
            </field>
        </record>


    </data>
</odoo>