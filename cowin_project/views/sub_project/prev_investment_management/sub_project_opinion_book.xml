<odoo>
    <data>
        <!-- 立项意见书-->


           <!--立项意见书的form视图-->
        <record model="ir.ui.view" id="sub_project_opinion_book_form">
            <field name="name">cowin_project.sub_opinion_book form</field>
            <field name="model">cowin_project.sub_opinion_book</field>
            <field name="arch" type="xml">
                <form string="Fiscal year" create="false" edit="false" delete="false">
                    <sheet>
                        <group>

                            <field name="name" readonly="1"/>
                            <field name="project_number" readonly="1"/>
                            <field name="invest_manager_ids" readonly="1"/>
                            <!--<field name="invest_manager_id" readonly="1"/>-->
                            <field name="date_of_project" readonly="1"/>
                            <!--投资基金-->
                            <field name="foundation_id" required="1" options='{"no_open":True}'/>
                            <field name="the_amount_of_investment" required="1"/>
                            <field name="ownership_interest" required="1"/>
                            <field name="round_financing_id" required="1" options='{"no_open":True}'/>
                            <field name="the_amount_of_financing" required="1"/>
                            <!---->
                            <field name="project_mumbers" required="1"/>
                            <!--<field name="partner_opinion"/>-->
                            <!--<field name="business_director_option"/>-->
                            <!--<field name="policy_making_committee"/>-->
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


    </data>
</odoo>