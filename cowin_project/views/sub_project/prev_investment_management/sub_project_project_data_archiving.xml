<odoo>
    <data>


        <!-- 项目资料归档-->


           <!--项目资料归档的form视图-->
        <record model="ir.ui.view" id="sub_project_data_archiving_form">
            <field name="name">cowin_project.sub_project_data_archiving form</field>
            <field name="model">cowin_project.sub_project_data_archiving</field>
            <field name="arch" type="xml">
                <form string="Fiscal year">
                    <group>

                        <field name="name" string="名称"/>
                        <field name="project_number"/>
                        <field name="invest_manager_id"/>
                        <field name="engagement_partner_id"/>
                        <!--投资基金-->
                        <field name="foundation_id"/>
                        <field name="the_amount_of_investment"/>
                        <field name="ownership_interest"/>
                        <field name="round_financing_id"/>
                        <field name="the_amount_of_financing"/>
                        <!---->
                        <field name="investment_decision_process_information" widget="many2many_binary"/>
                        <field name="relevant_legal_documents_and_materials" widget="many2many_binary"/>
                        <field name="government_approval_materials" widget="many2many_binary"/>
                        <field name="payment_process_information" widget="many2many_binary"/>
                        <field name="business_change_data" widget="many2many_binary"/>
                    </group>

                </form>
            </field>
        </record>

    </data>

</odoo>