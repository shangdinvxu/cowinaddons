<odoo>

    <data>

        <!-- 付款申请表-->


           <!--付款申请表的form视图-->
        <record model="ir.ui.view" id="sub_project_payment_app_form_form">
            <field name="name">cowin_project.sub_payment_app_form form</field>
            <field name="model">cowin_project.sub_payment_app_form</field>
            <field name="arch" type="xml">
                <form string="Fiscal year">
                    <group>

                        <field name="name" string="名称"/>
                        <field name="project_number"/>
                        <field name="invest_manager_id"/>
                        <field name="date_of_application"/>
                        <field name="list_of_examination_and_approval_documents" widget="many2many_binary"/>
                        <field name="monetary_unit"/>
                        <field name="the_payment_amount"/>
                        <field name="total_investment"/>
                        <field name="number_of_periods"/>
                        <field name="accumulated_payment_amount"/>
                        <field name="payment_amount"/>
                        <field name="payee_name"/>
                        <field name="account_number"/>
                        <field name="bank_of_deposit"/>
                        <field name="funds_provided"/>
                        <!--投资基金-->
                        <field name="foundation_id"/>
                        <field name="the_amount_of_investment"/>
                        <field name="ownership_interest"/>
                        <field name="round_financing_id"/>
                        <field name="the_amount_of_financing"/>
                        <!---->
                        <field name="payment_account"/>
                    </group>

                </form>
            </field>
        </record>

    </data>
</odoo>