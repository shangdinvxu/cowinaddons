<odoo>
    <data>
        <!--投资决策委员会会议表决票-->
        <!--投资决策委员会会议表决票的form视图-->



        <record model="ir.ui.view" id="sub_conference_resolutions_form">
            <field name="name">cowin_project.sub_conference_resolutions form</field>
            <field name="model">cowin_project.sub_conference_resolutions</field>
            <field name="arch" type="xml">
                <form string="Fiscal year" create="false" edit="false" delete="false">
                    <header>
                        <button type="object" name="button_approval_flow_info"  args="[55 ,66, 77]"  string="审核" style="background-color:#00B0DD;border-color:#00B0DD;"/>
                    </header>
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="project_number"/>
                            <!--<field name="invest_manager_id"/>-->
                            <field name="invest_manager_ids" readonly="1"/>
                            <field name="voting_committee"/>
                            <field name="members_of_voting_committee_ids" readonly="1"/>
                            <field name="voting_opinion"/>
                            <field name="voter_id"/>
                            <!--<field name="voter"/>-->
                        </group>

                        <group>

                                <field name="sub_pro_approval_flow_settings_record_ids" readonly="1">
                                       <tree >
                                         <field name="approval_person_id" readonly="1" options='{"no_open":True}'/>
                                         <field name="approval_role_id" readonly="1"/>
                                         <field name="approval_result" readonly="1"/>
                                         <field name="approval_opinion" readonly="1"/>
                                       </tree>
                                   </field>
                            </group>


                        <group>
                            <!--<button type="object" name="button_approval_flow_info"  args="[55 ,66, 77]"  string="审核"/>-->
                        </group>
                    </sheet>
                </form>
            </field>
        </record>



        <record model="ir.ui.view" id="sub_conference_resolutions_form_no_button">
            <field name="name">cowin_project.sub_conference_resolutions form</field>
            <field name="model">cowin_project.sub_conference_resolutions</field>
            <field name="arch" type="xml">
                <form string="Fiscal year" create="false" edit="false" delete="false">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="project_number"/>
                            <!--<field name="invest_manager_id"/>-->
                            <field name="invest_manager_ids" readonly="1"/>
                            <field name="voting_committee"/>
                            <field name="members_of_voting_committee_ids" readonly="1"/>
                            <field name="voting_opinion"/>
                            <field name="voter_id"/>
                            <!--<field name="voter"/>-->
                        </group>


                        <group>

                                <field name="sub_pro_approval_flow_settings_record_ids" readonly="1">
                                       <tree >
                                         <field name="approval_person_id" readonly="1" options='{"no_open":True}'/>
                                         <field name="approval_role_id" readonly="1"/>
                                         <field name="approval_result" readonly="1"/>
                                         <field name="approval_opinion" readonly="1"/>
                                       </tree>
                                   </field>
                            </group>

                    </sheet>
                </form>
            </field>
        </record>

    </data>
</odoo>