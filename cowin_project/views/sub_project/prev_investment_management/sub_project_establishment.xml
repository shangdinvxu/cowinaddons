<!--# 项目立项  构建小项目-->
<odoo>
    <data>
         <!--项目立项的form视图-->
        <record model="ir.ui.view" id="sub_project_establishment_form">
            <field name="name">cowin_project.cowin_subproject form</field>
            <field name="model">cowin_project.cowin_subproject</field>
            <field name="arch" type="xml">
                <form string="Fiscal year" create="false" edit="false" delete="false">
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="image" widget="image" class="oe_avatar" options="{'size': [90, 90]}"/>
                                <field name="project_number"/>
                                <field name="project_source"/>
                                <field name="project_source_note"/>
                                <field name="invest_manager_id"/>
                                <field name="project_company_profile"/>
                                <field name="project_appraisal"/>
                                <field name="project_note"/>
                                <field name="industry"/>
                                <field name="stage"/>
                                <field name="production"/>
                                <field name="registered_address"/>
                                <field name="peration_place"/>
                                <field name="founding_time"/>
                                <field name="contract_person"/>
                                <field name="contract_phone"/>
                                <field name="contract_email"/>
                                <field name="attachment_ids"  widget="many2many_binary"/>
                                <field name="attachment_note"/>
                            </group>


                            <group>
                                 <!--投资基金-->
                                <field name="foundation_id"/>
                                <field name="the_amount_of_investment"/>
                                <field name="ownership_interest"/>
                                <field name="round_financing_id"/>
                                <field name="the_amount_of_financing"/>
                                <!---->
                            </group>
                        </group>
                    </sheet>


                </form>
            </field>
        </record>

    </data>
</odoo>