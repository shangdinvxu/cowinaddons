

<odoo>

    <data>
        <!-- 投资项目年度分析报告-->


           <!--投资项目年度分析报告的form视图-->
        <record model="ir.ui.view" id="sub_project_annual_analysis_report_on_investment_projects_form">
            <field name="name">cowin_project.sub_app_for_investment_withdrawal form</field>
            <field name="model">cowin_project.sub_app_for_investment_withdrawal</field>
            <field name="arch" type="xml">
                <form string="Fiscal year">
                    <group>

                        <field name="name" string="名称"/>
                        <field name="reporter"/>
                        <field name="report_date"/>

                        <!--投资基金-->
                        <field name="foundation_id"/>
                        <field name="the_amount_of_investment"/>
                        <field name="ownership_interest"/>
                        <field name="round_financing_id"/>
                        <field name="the_amount_of_financing"/>
                        <!---->

                        <field name="trustee"/>
                        <field name="supervisor"/>
                        <field name="withdrawal_amount"/>
                        <field name="withdrawal_ratio"/>
                        <field name="decision_file_list" widget="many2many_binary"/>
                        <field name="exit_plan"/>

                    </group>

                </form>
            </field>
        </record>

    </data>
</odoo>