<odoo>
    <data>

        <!-- 三会授权书-->
        <!--三会授权书的form视图-->
        <record model="ir.ui.view" id="sub_three_empowermentg_form">
            <field name="name">cowin_project.sub_three_empowerment form</field>
            <field name="model">cowin_project.sub_three_empowerment</field>
            <field name="arch" type="xml">
                <form string="Fiscal year">

                    <sheet>
                        <group>
                            <group>
                                <field name="name" readonly="1"/>
                                <field name="topic_for_discussion"/>
                                <field name="managing_partner_ids" readonly="1"/>
                            </group>
                            <group>
                                <!--投资基金-->
                                <field name="foundation_id" readonly="1"/>
                                <field name="the_amount_of_investment" readonly="1"/>
                                <field name="ownership_interest" readonly="1"/>
                                <field name="round_financing_id" readonly="1"/>
                                <field name="the_amount_of_financing" readonly="1"/>
                                <!---->
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

    </data>
</odoo>