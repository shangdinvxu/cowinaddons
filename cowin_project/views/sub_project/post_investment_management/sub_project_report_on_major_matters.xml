<odoo>

    <data>
         <!-- 重大事项报告-->
        <!--重大事项报告的form视图-->
        <record model="ir.ui.view" id="sub_report_on_major_matters_form">
            <field name="name">cowin_project.sub_report_on_major_matters form</field>
            <field name="model">cowin_project.sub_report_on_major_matters</field>
            <field name="arch" type="xml">
                <form string="Fiscal year" create="false" edit="false" delete="false">
                    <sheet>
                        <group>
                            <group>
                                <field name="name" readonly="1"/>
                                <field name="project_number" readonly="1"/>
                                <field name="reporter" readonly="1"/>
                                <field name="filing_date" readonly="1"/>
                                <field name="key_words"/>
                                <field name="contents_of_matter"/>
                                <field name="enormous_impact"/>
                            </group>
                            <group>
                                <!--投资基金-->
                                <field name="foundation_id" readonly="1"/>
                                <field name="the_amount_of_investment" readonly="1"/>
                                <field name="ownership_interest" readonly="1"/>
                                <field name="round_financing_id" readonly="1"/>
                                <field name="the_amount_of_financing" readonly="1"/>
                                <!---->
                            </group>

                            <group>

                                <field name="sub_pro_approval_flow_settings_record_ids" readonly="1">
                                   <tree >
                                     <field name="approval_person_id" readonly="1" options='{"no_open":True}'/>
                                     <field name="approval_role_id" readonly="1"/>
                                     <field name="approval_result" readonly="1"/>
                                     <field name="approval_opinion" readonly="1"/>
                                   </tree>
                                </field>
                            </group>

                            <group>
                                <button type="object" name="button_approval_flow_info"  args="[55 ,66, 77]"  string="审核"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="sub_report_on_major_matters_form_no_button">
            <field name="name">cowin_project.sub_report_on_major_matters form</field>
            <field name="model">cowin_project.sub_report_on_major_matters</field>
            <field name="arch" type="xml">
                <form string="Fiscal year" create="false" edit="false" delete="false">
                    <sheet>
                        <group>
                            <group>
                                <field name="name" readonly="1"/>
                                <field name="project_number" readonly="1"/>
                                <field name="reporter" readonly="1"/>
                                <field name="filing_date" readonly="1"/>
                                <field name="key_words"/>
                                <field name="contents_of_matter"/>
                                <field name="enormous_impact"/>
                            </group>
                            <group>
                                <!--投资基金-->
                                <field name="foundation_id" readonly="1"/>
                                <field name="the_amount_of_investment" readonly="1"/>
                                <field name="ownership_interest" readonly="1"/>
                                <field name="round_financing_id" readonly="1"/>
                                <field name="the_amount_of_financing" readonly="1"/>
                                <!---->
                            </group>

                            <group>

                                <field name="sub_pro_approval_flow_settings_record_ids" readonly="1">
                                 <tree >
                                    <field name="approval_person_id" readonly="1" options='{"no_open":True}'/>
                                    <field name="approval_role_id" readonly="1"/>
                                    <field name="approval_result" readonly="1"/>
                                    <field name="approval_opinion" readonly="1"/>
                                </tree>
                            </field>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


    </data>
</odoo>