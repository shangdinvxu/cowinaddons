<odoo>
    <data>

        <!-- 项目公司三会纪要-->
        <!--项目公司三会纪要的form视图-->
        <record model="ir.ui.view" id="sub_sum_of_the_three_meeting_form">
            <field name="name">cowin_project.sub_sum_of_the_three_meeting form</field>
            <field name="model">cowin_project.sub_sum_of_the_three_meeting</field>
            <field name="arch" type="xml">
                <form string="Fiscal year" create="false" edit="false" delete="false">
                    <sheet>
                        <group>
                            <group>
                                <field name="name" readonly="1"/>
                                <field name="project_number" readonly="1"/>
                                <field name="reporter" readonly="1"/>
                                <field name="filing_date" readonly="1"/>
                                <field name="form_of_mettings"/>
                                <field name="mettings_summary"/>
                            </group>
                            <group>
                                <!--投资基金-->
                                <field name="foundation_id" readonly="1"/>
                                <field name="the_amount_of_investment" readonly="1"/>
                                <field name="ownership_interest" readonly="1"/>
                                <field name="round_financing_id" readonly="1"/>
                                <field name="the_amount_of_financing" readonly="1"/>
                                <!---->
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="sub_sum_of_the_three_meeting_form_no_button">
            <field name="name">cowin_project.sub_sum_of_the_three_meeting form</field>
            <field name="model">cowin_project.sub_sum_of_the_three_meeting</field>
            <field name="arch" type="xml">
                <form string="Fiscal year" create="false" edit="false" delete="false">
                    <sheet>
                        <group>
                            <group>
                                <field name="name" readonly="1"/>
                                <field name="project_number" readonly="1"/>
                                <field name="reporter" readonly="1"/>
                                <field name="filing_date" readonly="1"/>
                                <field name="form_of_mettings"/>
                                <field name="mettings_summary"/>
                            </group>
                            <group>
                                <!--投资基金-->
                                <field name="foundation_id" readonly="1"/>
                                <field name="the_amount_of_investment" readonly="1"/>
                                <field name="ownership_interest" readonly="1"/>
                                <field name="round_financing_id" readonly="1"/>
                                <field name="the_amount_of_financing" readonly="1"/>
                                <!---->
                            </group>

                            <group>
                                <button type="object" name="button_approval_flow_info"  args="[55 ,66, 77]"  string="审核"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>