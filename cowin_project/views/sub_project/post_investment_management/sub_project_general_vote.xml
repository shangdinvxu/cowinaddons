<odoo>

    <data>

         <!-- 一般事项表决-->
        <!--一般事项表决的form视图-->
        <record model="ir.ui.view" id="sub_general_vote_form">
            <field name="name">cowin_project.sub_general_vote form</field>
            <field name="model">cowin_project.sub_general_vote</field>
            <field name="arch" type="xml">
                <form string="Fiscal year">
                    <group>

                        <field name="name"/>
                        <field name="project_number"/>
                        <field name="invest_manager_id"/>
                        <field name="reporter"/>
                        <field name="date_convening"/>
                        <field name="instructions_voting"/>

