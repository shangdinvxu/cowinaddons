<odoo>

    <data>

         <!-- 项目退出决议-->
        <!--项目退出决议的form视图-->
        <record model="ir.ui.view" id="sub_project_exit_resolution_form">
            <field name="name">cowin_project.sub_project_exit_resolution form</field>
            <field name="model">cowin_project.sub_project_exit_resolution</field>
            <field name="arch" type="xml">
                <form string="Fiscal year">
                    <sheet>
                        <group>
                            <group>
                                <field name="name" readonly="1"/>
                                <field name="project_number" readonly="1"/>
                                <field name="invest_manager_id" readonly="1"/>
                                <field name="project_exit_date" readonly="1"/>
                                <field name="result" readonly="1"/>
                                <field name="withdrawal_amount"/>
                                <field name="withdrawal_ratio"/>
                                <field name="exit_plan"/>
                                <field name="chairman_of_investment_decision_committee" readonly="1"/>
                            </group>
                            <group>
                                 <!--投资基金-->
                                <field name="foundation_id" readonly="1"/>
                                <field name="the_amount_of_investment" readonly="1"/>
                                <field name="ownership_interest" readonly="1"/>
                                <field name="round_financing_id" readonly="1"/>
                                <field name="the_amount_of_financing" readonly="1"/>
                                <!---->
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


    </data>
</odoo>