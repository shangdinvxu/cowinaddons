<odoo>

    <data>

         <!-- 项目退出决议-->
        <!--项目退出决议的form视图-->
        <record model="ir.ui.view" id="sub_project_exit_resolution_form">
            <field name="name">cowin_project.sub_project_exit_resolution form</field>
            <field name="model">cowin_project.sub_project_exit_resolution</field>
            <field name="arch" type="xml">
                <form string="Fiscal year">
                    <group>

                        <field name="name"/>
                        <field name="project_number"/>
                        <!--<field name="invest_manager_id"/>-->
                        <field name="invest_manager_ids"/>
                        <field name="project_exit_date"/>
                        <field name="result"/>

                        <!--投资基金-->
                        <field name="foundation_id"/>
                        <field name="the_amount_of_investment"/>
                        <field name="ownership_interest"/>
                        <field name="round_financing_id"/>
                        <field name="the_amount_of_financing"/>
                        <!---->
                        <field name="withdrawal_amount"/>
                        <field name="withdrawal_ratio"/>
                        <field name="exit_plan"/>
                        <!--<field name="chairman_of_investment_decision_committee"/>-->
                        <field name="chairman_of_investment_decision_committee_ids"/>


                    </group>

                </form>
            </field>
        </record>


    </data>
</odoo>