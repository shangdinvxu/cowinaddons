<odoo>

    <data>


         <!-- 重大事项表决-->
        <!--重大事项表决的form视图-->
        <record model="ir.ui.view" id="sub_vote_on_major_matters_form">
            <field name="name">cowin_project.sub_vote_on_major_matters form</field>
            <field name="model">cowin_project.sub_vote_on_major_matters</field>
            <field name="arch" type="xml">
                <form string="Fiscal year">
                    <sheet>
                        <group>
                            <group>
                                <field name="name" readonly="1"/>
                                <field name="project_number" readonly="1"/>
                                <field name="reporter" readonly="1"/>
                                <field name="date_convening" readonly="1"/>
                                <field name="instructions_voting"/>
                            </group>
                            <group>
                                <!--投资基金-->
                                <field name="foundation_id" readonly="1"/>
                                <field name="the_amount_of_investment" readonly="1"/>
                                <field name="ownership_interest" readonly="1"/>
                                <field name="round_financing_id" readonly="1"/>
                                <field name="the_amount_of_financing" readonly="1"/>
                                <!---->
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


    </data>
</odoo>