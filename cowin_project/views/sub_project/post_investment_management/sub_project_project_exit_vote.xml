<odoo>

    <data>


        <!-- 项目退出会议表决票-->
        <!--项目退出会议表决票的form视图-->
        <record model="ir.ui.view" id="sub_exit_vote_form">
            <field name="name">cowin_project.sub_exit_vote form</field>
            <field name="model">cowin_project.sub_exit_vote</field>
            <field name="arch" type="xml">
                <form string="Fiscal year" create="false" edit="false" delete="false">
                    <header>
                        <button type="object" name="button_approval_flow_info"  args="[55 ,66, 77]"  string="审核" style="background-color:#00B0DD;border-color:#00B0DD;"/>
                    </header>
                    <sheet>
                        <group>

                            <field name="name"/>
                            <field name="project_number"/>
                            <field name="invest_manager_ids"/>

                            <!--投资基金-->
                            <field name="foundation_id"/>
                            <field name="the_amount_of_investment"/>
                            <field name="ownership_interest"/>
                            <field name="round_financing_id"/>
                            <field name="the_amount_of_financing"/>
                            <!---->
                            <field name="conference_date"/>
                            <!--<field name="investment_decision_committee"/>-->
                            <field name="members_of_voting_committee_ids"/>
                            <field name="voting_result"/>
                            <field name="voting_opinion"/>
                            <field name="voter"/>


                        </group>

                        <group>

                            <field name="sub_pro_approval_flow_settings_record_ids" readonly="1">
                                   <tree >
                                     <field name="approval_person_id" readonly="1" options='{"no_open":True}'/>
                                     <field name="approval_role_id" readonly="1"/>
                                     <field name="approval_result" readonly="1"/>
                                     <field name="approval_opinion" readonly="1"/>
                                   </tree>
                               </field>
                        </group>
                    </sheet>




                    <!--<group>-->
                        <!--<button type="object" name="button_approval_flow_info"  args="[55 ,66, 77]"  string="审核"/>-->
                    <!--</group>-->

                </form>
            </field>
        </record>


        <record model="ir.ui.view" id="sub_exit_vote_form_no_button">
            <field name="name">cowin_project.sub_exit_vote form</field>
            <field name="model">cowin_project.sub_exit_vote</field>
            <field name="arch" type="xml">
                <form string="Fiscal year" create="false" edit="false" delete="false">
                    <group>

                        <field name="name"/>
                        <field name="project_number"/>
                        <field name="invest_manager_ids"/>

                        <!--投资基金-->
                        <field name="foundation_id"/>
                        <field name="the_amount_of_investment"/>
                        <field name="ownership_interest"/>
                        <field name="round_financing_id"/>
                        <field name="the_amount_of_financing"/>
                        <!---->
                        <field name="conference_date"/>
                        <!--<field name="investment_decision_committee"/>-->
                        <field name="members_of_voting_committee_ids"/>
                        <field name="voting_result"/>
                        <field name="voting_opinion"/>
                        <field name="voter"/>


                    </group>

                    <group>

                        <field name="sub_pro_approval_flow_settings_record_ids" readonly="1">
                               <tree >
                                 <field name="approval_person_id" readonly="1" options='{"no_open":True}'/>
                                 <field name="approval_role_id" readonly="1"/>
                                 <field name="approval_result" readonly="1"/>
                                 <field name="approval_opinion" readonly="1"/>
                               </tree>
                           </field>
                    </group>

                </form>
            </field>
        </record>


    </data>
</odoo>