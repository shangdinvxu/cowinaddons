<odoo>

    <data>


        <!-- 项目退出会议表决票-->
        <!--项目退出会议表决票的form视图-->
        <record model="ir.ui.view" id="sub_exit_vote_form">
            <field name="name">cowin_project.sub_exit_vote form</field>
            <field name="model">cowin_project.sub_exit_vote</field>
            <field name="arch" type="xml">
                <form string="Fiscal year">
                    <sheet>
                        <group>
                            <group>
                                <field name="name" readonly="1"/>
                                <field name="project_number" readonly="1"/>
                                <field name="invest_manager_id" readonly="1"/>
                                <field name="conference_date" readonly="1"/>
                                <field name="investment_decision_committee" readonly="1"/>
                                <field name="voting_result"/>
                                <field name="voting_opinion"/>
                                <field name="voter"/>
                            </group>
                            <group>
                                <!--投资基金-->
                                <field name="foundation_id" readonly="1"/>
                                <field name="the_amount_of_investment" readonly="1"/>
                                <field name="ownership_interest" readonly="1"/>
                                <field name="round_financing_id" readonly="1"/>
                                <field name="the_amount_of_financing" readonly="1"/>
                                <!---->
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

    </data>
</odoo>