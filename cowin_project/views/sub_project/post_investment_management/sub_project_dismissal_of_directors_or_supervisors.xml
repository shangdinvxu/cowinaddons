<odoo>

    <data>

        <!-- 董事／监事解聘书-->
        <!--董事／监事解聘书的form视图-->
        <record model="ir.ui.view" id="sub_dismissal_of_directs_or_supers_form">
            <field name="name">cowin_project.sub_dismissal_of_directs_or_supers form</field>
            <field name="model">cowin_project.sub_dismissal_of_directs_or_supers</field>
            <field name="arch" type="xml">
                <form string="Fiscal year"  create="false" edit="false" delete="false">
                    <group>

                        <field name="name"/>
                        <field name="invest_manager_ids"/>
                        <field name="trustee_id"/>
                        <field name="appointment_time_begin_trustee"/>
                        <field name="appointment_time_end_trustee"/>
                        <field name="Tenure_trustee"/>
                        <field name="supervisor_id"/>
                        <field name="appointment_time_begin_supervisor"/>
                        <field name="appointment_time_end_supervisor"/>
                        <field name="Tenure_supervisor"/>
                        <field name="managing_partner_ids"/>

                        <!--投资基金-->
                        <field name="foundation_id"/>
                        <field name="the_amount_of_investment"/>
                        <field name="ownership_interest"/>
                        <field name="round_financing_id"/>
                        <field name="the_amount_of_financing"/>
                        <!---->

                    </group>

                    <group>
                      <field name="sub_pro_approval_flow_settings_record_ids" >
                          <tree >
                              <field name="approval_person_id" readonly="1" options='{"no_open":True}'/>
                              <field name="approval_role_id" readonly="1"/>
                              <field name="approval_result" readonly="1"/>
                              <field name="approval_opinion" readonly="1"/>
                          </tree>

                      </field>

                    </group>

                    <group>
                        <button type="object" name="button_approval_flow_info"  args="[55 ,66, 77]"  string="审核"/>
                    </group>

                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="sub_dismissal_of_directs_or_supers_form_no_button">
            <field name="name">cowin_project.sub_dismissal_of_directs_or_supers form</field>
            <field name="model">cowin_project.sub_dismissal_of_directs_or_supers</field>
            <field name="arch" type="xml">
                <form string="Fiscal year"  create="false" edit="false" delete="false">
                    <group>

                        <field name="name"/>
                        <field name="invest_manager_ids"/>
                        <field name="trustee_id"/>
                        <field name="appointment_time_begin_trustee"/>
                        <field name="appointment_time_end_trustee"/>
                        <field name="Tenure_trustee"/>
                        <field name="supervisor_id"/>
                        <field name="appointment_time_begin_supervisor"/>
                        <field name="appointment_time_end_supervisor"/>
                        <field name="Tenure_supervisor"/>
                        <field name="managing_partner_ids"/>

                        <!--投资基金-->
                        <field name="foundation_id"/>
                        <field name="the_amount_of_investment"/>
                        <field name="ownership_interest"/>
                        <field name="round_financing_id"/>
                        <field name="the_amount_of_financing"/>
                        <!---->

                         <group>
                            <field name="sub_pro_approval_flow_settings_record_ids" >
                                <tree >
                                  <field name="approval_person_id" readonly="1" options='{"no_open":True}'/>
                                  <field name="approval_role_id" readonly="1"/>
                                  <field name="approval_result" readonly="1"/>
                                  <field name="approval_opinion" readonly="1"/>
                                </tree>

                            </field>

                         </group>

                    </group>

                </form>
            </field>
        </record>


    </data>
</odoo>