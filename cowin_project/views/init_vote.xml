<odoo>
    <data>


        <!--投资决策委员会会议表决票-->
        <!--投资决策委员会会议表决票的tree视图-->
        <record model="ir.ui.view" id="sub_project_conference_resolutions_list">
            <field name="name">cowin_project.sub_conference_resolutions form</field>
            <field name="model">cowin_project.sub_conference_resolutions</field>
            <field name="arch" type="xml">
                <tree create="false" edit="false" delete="false">
                    <field name="name"/>
                    <field name="project_number"/>
                    <field name="foundation_id"/>
                    <field name="round_financing_id"/>
                    <field name="the_amount_of_investment"/>
                    <field name="the_amount_of_financing"/>
                    <field name="ownership_interest"/>
                </tree>
            </field>
        </record>


         <!-- 我的投票 -->
        <record model="ir.actions.act_window" id="sub_conference_resolutions_action_window">
            <field name="name">我的投票</field>
            <field name="res_model">cowin_project.sub_conference_resolutions</field>
             <field name="view_ids"
                   eval="[(5, 0, 0),
                          (0, 0, {'view_mode': 'tree', 'view_id': ref('sub_project_conference_resolutions_list')})]"/>
            <field name="view_mode">tree,form</field>
        </record>


        <menuitem name="我的投票" id="cowin_project.menu_24" parent="cowin_project.menu_4"
                  action="sub_conference_resolutions_action_window"/>

        <menuitem name="投票查询" id="cowin_project.menu_25" parent="cowin_project.menu_4"
                  action="sub_conference_resolutions_action_window"/>

    </data>


</odoo>