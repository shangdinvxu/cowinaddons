<odoo>
    <data>
         <template id="cowin_hr_static" name="test_view" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <!--<script type="text/javascript" src="/linkloving_project/static/src/js/new_gantt.js"/>-->
                <!--<script type="text/javascript" src="/linkloving_project/static/src/js/dhtmlxgantt.js"/>-->
                <script type="text/javascript" src="/cowin_hr/static/src/js/employee_view.js"/>
                <link rel="stylesheet" href="/cowin_hr/static/src/css/employee_view.css"/>
            </xpath>
        </template>





        <template id="menu_link_inherit" inherit_id="web.menu_link" name="menu Inherit">

            <xpath expr="//a/span[@class='oe_menu_text']" position="replace">
                <span class="oe_menu_text">
                    <span t-att-class="menu['button_icon']"></span>
                    <!--<div data-toggle="tooltip" data-placement="right" t-att-title="menu['tip_text']">-->
                    <t t-esc="menu['name']"/>
                    <!--</div>-->
                </span>
            </xpath>
        </template>



        <record id="base.edit_menu_access" model="ir.ui.view">
            <field name="model">ir.ui.menu</field>
            <field name="arch" type="xml">
                <form string="Menu">
                  <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="button_icon"/>
                            <field name="parent_id" groups="base.group_no_one"/>
                            <field name="sequence" groups="base.group_no_one"/>
                        </group>
                        <group groups="base.group_no_one">
                            <field name="complete_name"/>
                            <field name="action"/>
                            <field name="web_icon"/>
                        </group>
                    </group>
                    <notebook>
                        <page string="Access Rights">
                            <field name="groups_id"/>
                        </page>
                        <page string="Submenus" groups="base.group_no_one">
                            <!-- Note: make sure you have 'ir.ui.menu.full_list'
                                 in the context to see all submenus! -->
                            <field name="child_id"
                                    context="{'default_parent_id': active_id}">
                                <tree string="Menu">
                                    <field name="sequence"/>
                                    <field icon="icon" name="name" string="Menu"/>
                                </tree>
                            </field>
                        </page>
                    </notebook>
                   </sheet>
                </form>
            </field>
        </record>





    </data>
</odoo>